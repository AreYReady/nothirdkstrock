<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#fff4a60f" android:endColor="#ffe3a224" android:angle="90.0" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="90.0" />
        </shape>
    </item>
</selector>