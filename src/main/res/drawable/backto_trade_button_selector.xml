<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ff80ab16" android:endColor="#ff61850a" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff4f4f4f" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffffa508" android:endColor="#ffffbf50" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff4f4f4f" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>