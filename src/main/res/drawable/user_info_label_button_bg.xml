<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff729813" android:endColor="#ff506b0d" android:angle="90.0" />
            <corners android:radius="6.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ff9fbf50" android:endColor="#ff749b15" android:angle="270.0" />
            <corners android:radius="6.0dip" />
        </shape>
    </item>
</selector>