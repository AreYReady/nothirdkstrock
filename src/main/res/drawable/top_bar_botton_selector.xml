<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffff7f00" android:endColor="#ffffa800" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff000000" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ff525252" android:endColor="#ff242424" android:angle="270.0" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1.0dip" android:color="#ff000000" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_pressed="false">
        <shape>
            <solid android:color="#ff242424" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1.0dip" android:color="#ff000000" />
        </shape>
    </item>
</selector>